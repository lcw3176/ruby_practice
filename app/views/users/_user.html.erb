<div id="<%= dom_id user %>">
  <p>
    <strong>Username:</strong>
    <%= user.username %>
  </p>

  <p>
    <strong>Password:</strong>
    <%= user.password %>
  </p>

  <p>
    <strong>Salt:</strong>
    <%= user.salt %>
  </p>

  <p>
    <strong>Email:</strong>
    <%= user.email %>
  </p>

  <p>
    <strong>Dm:</strong>
    <%= user.dm %>
  </p>

  <p>
    <strong>Roles:</strong>
    <%= user.roles %>
  </p>

  <p>
    <strong>Reviews count:</strong>
    <%= user.reviews_count %>
  </p>

</div>
